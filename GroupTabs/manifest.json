{
  "manifest_version": 2,
  "name": "GroupTabs",
  "description": "This extension groups all tabs from the same domain into one window",
  "icons": {
    "48": "icon.png"
  },
  "version": "1.0",
  "background": {
    "persistent": true,
    "scripts": ["background.js","scripts/jquery-3.2.1.min.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "GroupTabs",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "permissions": ["tabs","contextMenus","storage"]
}
